  background: {{settings.content_bg_colour}};
}
.browse-tags{line-height: 30px; margin-left: 20px;}
.browse-tags label{float: left; margin-right: 10px;}
.browse-tags select{float: left;}

#collection_tags{
  float: left;
  border: 1px solid {{settings.content_secondary_colour}};
  padding: 5px 10px;
  margin-bottom: 10px;
  height: 30px;
  line-height: 30px;
  border-radius: 15px;
  -moz-border-radius: 15px;
  -webkit-border-radius: 15px;

  -webkit-box-shadow: inset {{settings.content_secondary_colour}} 0px 2px 3px;
  -moz-box-shadow: inset {{settings.content_secondary_colour}} 0px 2px 3px;
  box-shadow: inset {{settings.content_secondary_colour}} 0px 2px 3px;
}
#sharing ul li  ,#sharing ul li *{
  float: letf;
  margin: 0 !important;
}
#product-information #sharing {
  border-bottom: 1px solid {{settings.content_secondary_colour}} !important;
  border-top: none;
  padding: 0 0 20px;
}
#product-information #sharing ul{margin: 0; width: 100%;}


#product-header{
  padding: 20px !important;
}
/*end detail product*/
/*connection*/
/*=================*/
ul#featured-collections li{
  width: 294.9px;
  height: 375px;
  display: table_cell;
  overflow: hidden;
  line-height: 20px;
}
.collection-grid-item{
  border: 1px solid {{settings.content_secondary_colour}};
  display: table-cell;
  height: 348px;
  position: relative;
  vertical-align: middle;
  width: 298px;
  float:left;

}
.collection-grid-item .collection-image{
  height: 348px;
}
.collection-photo{
  display: inline-block;
}

.collection-grid-item  img{
  margin: 0 auto;
  display: block;
  max-width:100%;
  width: 100%;
  max-height:350px;
}
.collection-information{
  text-align: center;
  display: none;
  border-bottom: 1px solid {{settings.content_secondary_colour}};
  cursor: default;
  z-index:10;
  position:absolute;
  left:0;
  top:auto !important;
  bottom:0;
  width:100%;

}

.collection-outer{
  background: {{settings.prod_info_bg}};
  padding: 25px 0 20px;
}
span.collection-title{
  color: {{settings.content_header_colour}};
  display: block;
  font-family: arial;
  font-size: 20px;
  font-weight: 400;
  text-transform: uppercase;
  padding-top: 5px;
  padding-bottom: 10px;
}

.shop-collection a{
  color: {{settings.content_link_colour}};
  font-size: 14px;
  font-weight:bold;
  text-decoration: none;
  font-family: {{settings.general_font}} !important;
}
.shop-collection a:hover{color: {{settings.content_link_hover_colour}};}

/*=====================*/
/*banner*/
.wrap_banner img{border-bottom: 1px solid {{settings.page_tt_border}};}
.variants-wrapper select.single-option-selector, span.custom-style-select-box{min-width: 70px;}
.variants-wrapper select.single-option-selector,span.custom-style-select-box{
  appearance: none !important;
  -webkit-appearance: none !important;
  -moz-appearance: none !important;

  background: #ffffff url({{'select.png' | asset_url}}) no-repeat 100% center;
  padding-right: 30px;
  border: 1px solid {{settings.content_secondary_colour}};
}
input.item-quantity{padding-right: 30px; border: 1px solid {{settings.content_secondary_colour}}; appearance: none !important;-webkit-appearance: none !important; -moz-appearance: none !important; line-height: 15px;
  padding: 6px 20px 6px 10px; width: 65px;}
input.item-quantity, span.custom-style-select-box{
  border-radius: 15px;
  -moz-border-radius: 15px;
  -webkit-border-radius: 15px;

  -webkit-box-shadow: inset {{settings.content_secondary_colour}} 0px 2px 3px;
  -moz-box-shadow: inset {{settings.content_secondary_colour}} 0px 2px 3px;
  box-shadow: inset {{settings.content_secondary_colour}} 0px 2px 3px;
  text-align: left;
  float: left;
}
span.custom-style-select-box{padding: 6px 20px 6px 4px; line-height: 15px;}
span.custom-style-select-box span{padding: 0 10px; width: auto !important;}
span.qty-group{
  position: relative;
  z-index:9;
  display: block;
  padding-right: 20px;
}
span.qty-up, span.qty-down{cursor: default; width: 20px; height: 10px; display: block; float: left; position: absolute; left: 140px; cursor: default; z-index: 1;}
span.qty-up{top: 4px; background: url({{'select_up.png' | asset_url}}) no-repeat 0 0;}
span.qty-down{ top: 15px; background: url({{'select_down.png' | asset_url}}) no-repeat 0 0;}


/*detail*/
.selector-wrapper .wrapper{width: 50%; float: left; position: relative;}
.selector-wrapper .single-option-selector,.selector-wrapper span.custom-style-select-box{position: absolute; left: 0px; top: 0;}
input.quantity{display: inline !important;}
