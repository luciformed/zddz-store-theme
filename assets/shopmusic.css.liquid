  	filter: alpha(opacity=50);
}
.next:focus, .prev:focus{outline: none;}

.button_categories{
    font-size: 14px; 
    color: {{settings.btn1_txt_clr}}; 
    display: inline-block; 
    background: {{settings.btn1_clr}}; 
    border-radius: 5px; 
    -moz-border-radius: 3px; 
    font-family: {{settings.general_font}};
    font-weight: bold;
    text-align: center; 
    text-transform: uppercase; 
    border: none;   
  	opacity: 1;
    border-bottom: 2px solid {{settings.btn1_border_clr}}; 	
    margin-bottom: 10px;
    cursor: pointer;
    padding: 7px 25px; 
    float: right;
    position: absolute;
    right: 0;
    top: -30px;
  	transition: all 0.3s;
	-webkit-transition: all 0.3s;
  	box-shadow: none;
}

.button_categories:hover{
  	background:{{settings.btn1_hover_clr}}; 
