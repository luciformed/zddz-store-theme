/*grid_list*/
.grid_list,.sort{
  float: right;
  margin-right: 20px;
  position: relative;
  z-index: 100;
}
.sort {
  margin-top: 1em;
  margin-bottom: 1em;
}
.sort .title{
  	font-size: 12px;
  	font-weight: normal;
  	color: {{settings.content_text_colour}};
    display: inline-block;
    font-family: arial;
    vertical-align: middle;
  	margin: 5px 10px 5px 0;
  	float: left;
}
.grid_list ul{list-style: none;}
.grid_list ul li{float:left;}
.grid_list ul li a span{
  	background:url({{'group_icon.png' | asset_url}}) no-repeat;
  	transition: all 0.3s;
	-webkit-transition: all 0.3s;
}
.grid_list ul li#lgrid a#gridview{padding: 8px 12px 8px 19px; display: block;
  	border-radius: 15px 0 0 15px;
	-moz- border-radius: 15px 0 0 15px;
}
.grid_list ul li#lgrid a#gridview span{
  	background-position: -122px -1px;
    display: block;
    height: 10px;
    width: 10px;
	text-indent: -9999px;
}
.grid_list ul li#lgrid.active a#gridview span, .grid_list ul li#lgrid:hover a#gridview span{
  	background-position: -136px -1px;
}
.grid_list ul li a{
  border: 1px solid {{settings.btn5_border_clr}};
  background: {{settings.btn5_clr}};
}
.grid_list ul li.active a, .grid_list ul li:hover a{
  	border: 1px solid {{settings.btn5_active_border_clr}};
  	background: {{settings.btn5_active_clr}};
}
.grid_list ul li.active a{
  	cursor: default;
}
.grid_list ul li#llist a#listview{
  	padding: 8px 19px 7px 12px;
  	display: block;
  	border-radius: 0 15px 15px 0;
  	-moz-border-radius: 0 15px 15px 0;
}
.grid_list ul li#llist a#listview span{
  	width: 12px;
  	height: 11px;
  	background-position: -148px -1px;
  	display: block;
  	text-indent: -9999px;
}
.grid_list ul li#llist.active a#listview span, .grid_list ul li#llist:hover a#listview span{
  	background-position: -164px -1px;
}
#sortBox{
  	background: none repeat scroll 0 0 #FFFFFF;
  	border: 1px solid {{settings.content_secondary_colour}};
    padding: 10px 0;
    position: absolute;
    right: -6px;
    top: 28px;
    width: 140px;
    z-index: 2;
  	border-top: 0;
}
#sortBox ul#sortForm{
  list-style: none;
  z-index: 1;
}
#sortBox ul#sortForm li{
  	float: left;
}
#sortBox ul#sortForm li:hover{
  	color: {{settings.content_link_hover_colour}};
}
#sortButton{
  	display: inline-block;
    float: right;
  	position: relative;
}
#sortButton i.icon-caret-down{
    position: absolute;
    top: 11px;
    right: 12px;
    font-size: 8px;
    cursor: pointer;
}
#sortButton label{
    border: 1px solid {{settings.btn4_border_clr}} !important;
  -webkit-box-shadow: 0 1px 3px {{settings.content_secondary_colour}} inset;
    -moz-box-shadow: 0 1px 3px {{settings.content_secondary_colour}} inset;
    box-shadow: 0 1px 3px {{settings.content_secondary_colour}} inset;
  	width: 100px;
    display: block;
    border-radius: 15px 15px 15px 15px;
    padding: 4px 20px;
  	text-align: left;
  	font-size: 12px;
  	font-family: arial;
  	color: {{settings.btn3_txt_clr}};
  	display: block;
  	float: left;
  	background:{{settings.btn3_clr}};
}
#sortForm li{
  	font-size: 12px;
  font-family:{{settings.general_font}};
  	color: {{settings.content_text_colour}};
  	cursor: pointer;
}
span.sortArrow{
  	position: absolute;
  	top: 13px;
  	right: 10px;
  	background:url({{'group_icon.png' | asset_url}}) no-repeat -15px -70px;
  	width: 6px;
  	height: 3px;
  	display: block;
}
.grid_list ul{
  	float: left;
}
.sort{
  	float: right;
}
/*end grid_list*/

