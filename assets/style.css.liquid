.add_form_email, #widget-mailing-list{
  	clear: both;
}
.add_form_email, #widget-mailing-list .contact-form{
    height:34px;  
    border-radius:5px;
    -moz-border-radius:5px;
    position: relative;
    padding: 0 85px 0 10px;
    vertical-align: middle;
    border: 1px solid {{settings.content_secondary_colour}};
}
#email-submit, #webform_submit_button {  
    border: medium none;
    position: absolute;
    text-align: right;
    right: 10px;
    top: 7px;
    
    width: 68px;
    height: 16px;
	font-size: 12px;  
    
    text-transform: uppercase;
    overflow: hidden;
  	color: {{settings.content_header_colour}}; 
    background: transparent;
    border-radius: 0 !important;
    padding: 0;
}


#email-input, #signup_email {
  background: none repeat scroll 0 0 transparent;
  border: medium none !important;
  color: {{settings.content_text_colour}};  
  width: 180px;
  line-height: 20px;
  margin: 0;    
  padding: 7px 0;
  font-size: 12px;  
  cursor: text;
  outline:none !important; 
  border: none !important; 
  box-shadow: none !important;
  position: absolute;
  top: 2px;
  left: 10px;
}
#email-input:focus, #signup_email {outline:none !important; border: none !important; box-shadow: none !important;}

#widget-mailing-list .contact-form.submitted {
  border: 0;
  padding: 0;
}
  

