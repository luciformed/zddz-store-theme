/**
* Pagination
* Modified from http://twitter.github.com/bootstrap/
*/


  

.pagination {  
  padding: 0px 0 70px 0;
  margin: 0 20px;
  text-align: center;
  position: relative;
  overflow: hidden;
}
.pagination ul {      
  background: {{ settings.pagination_background }};
  border-radius: 15px;
  -moz-border-radius: 15px;
  list-style: none outside none;
  font-size: 0;
  margin: 0 auto;
  clear: both;
  
}

.pagination li.page-next, .pagination li.page-next.disabled{
  	border-radius: 0 15px 15px 0;
  	-moz-border-radius: 0 15px 15px 0;  	
  	border: 1px solid {{ settings.pagination_border }};  	  	
}
.pagination li.page-prev, .pagination li.page-prev.disabled{
  	border-radius: 15px 0 0 15px;
  	-moz-border-radius: 15px 0 0 15px; 
  	border-top: 1px solid {{ settings.pagination_border }};
  	border-bottom: 1px solid {{ settings.pagination_border }};
  	border-left: 1px solid {{ settings.pagination_border }};  	
  
}
.pagination li.page-prev.disabled{
  	cursor: default;
  	
}
.pagination li {
  display: inline-block;
  line-height: 16px;
  border-top: 1px solid {{ settings.pagination_border }};
  border-bottom: 1px solid {{ settings.pagination_border }};
  border-left: 1px solid {{ settings.pagination_border }};
  padding: 0 16px;
}
.pagination a { 
  font-size: 14px;
  line-height: 26px;
  text-decoration: none;
  color: {{ settings.pagination_link}};
  vertical-align: middle;
}
.pagination .active{
  	cursor: default !important;
}
.pagination li:hover a, .pagination .active a {
  font-weight: bold;
  color: {{ settings.pagination_link_hover}};
}

.pagination .disabled, .pagination .disabled:hover {
  background-color: {{ settings.pagination_border }};
}
.pagination .disabled a, .pagination .disabled:hover a{  
  cursor: default;
  font-weight: normal;
  color: {{ settings.pagination_link_hover}};
  
}
.pagination li{height: 30px;}
.pagination li:hover{cursor: pointer;}
.pagination li.active a:hover{cursor: default;}
.pagination li a{
  	padding: 3px 0; 
  	vertical-align: middle; 
  	line-height: 23px;
  	display: inline-block;
}
.pagination ul li.disabled{
  	opacity: 1 !important;
}
.pagination-num-showing{ 
  text-align: center;
  float: right;
  margin-right: 20px;
  color: {{settings.pagination_link}};
  line-height: 28px;
  font-size: 12px;
  color: {{ settings.pagination_link}};
  position: absolute;
  right: 0;
  top: 0;
}
.pagination li.next-post a,
.pagination li.prev-post a{
  	font-size: 12px;
}

.pagination li.next-post,
.pagination li.prev-post{ 
  	border-radius: 15px;
  	-moz-border-radius: 15px;
  	background: #fff; 
  	color: {{settings.content_text_colour}}; 
  	border: 1px solid {{ settings.pagination_border }};
