{% layout none %}
<!DOCTYPE html>
<html>

<head>
  <title>ZDDZ Shop | Coming Soon...</title>
  <style type="text/css">
    * {
      margin: 0;
      padding: 0;
    }

    img {
      border: 0;
    }

    body {
      margin: 0;
      background-color: #e0e0e0;
      font: 1em/1.4em "Lucida Grande", "Trebuchet MS", Helvetica, sans-serif;
      color: #333;
    }

    h1,h2,h3,h4,h5,h6 {
      color: #000;
      margin: 5px 0 10px 0;
      font-weight: 400;
    }

    h3,h4,h5,h6 {
      font-weight: 700;
    }

    a {
      color: #3084CA;
    }

    a:hover {
      color: #25669C;
    }

    p {
      color: #666;
      margin-bottom: 13px;
      font-size: 14px;
    }

    ul, ol {
      margin-bottom: 13px;
      margin-left: 15px;
    }

    ol {
      margin-left: 25px;
    }

    input.btn {
      font-weight: bold;
      cursor: pointer;
      border: 1px solid #bbb;
      border-top: 1px solid #ccc;
      border-bottom: 1px solid #aaa;
      background-color: #ddd;
      background-image: url('data:image/jpeg;base64,');
      background-position: top left;
      background-repeat: repeat-x;
      padding: 4px 6px;
      -moz-border-radius: 5px;
      -webkit-border-radius: 5px;
      border-radius: 5px;
      text-shadow: 0px 1px 1px #fff;
      color: #3a3a3a;
    }

    input.btn:hover {
      -webkit-box-shadow: 0px 0px 3px #3084CA;
      -moz-box-shadow: 0px 0px 3px  #3084CA;
    }

    input.btn:active, input.btn:focus {
      outline: none;
      -moz-outline-style: none;
      background: #eee;
      border-top: 1px solid #aaa;
      border-bottom: 1px solid #ccc;
      color: #555;
    }

    #container {
      width: 500px;
      margin: 50px auto 0 auto;
    }

    #login {
      background: #fff;
      border: none;
      border-bottom: 1px solid #ccc;
      margin: 50px auto 0 auto;
      padding: 30px;
      width: 450px;
      -webkit-border-radius: 4px;
      -moz-border-radius: 4px;
      border-radius: 4px;
      box-shadow: 0 0 10px rgba(0,0,0,0.1);
      -moz-box-shadow: 0 0 10px rgba(0,0,0,0.1);
      -webkit-box-shadow: 0 0 10px rgba(0,0,0,0.1);
      box-shadow: 0 0 10px rgba(0,0,0,0.1);
    }

    #login h1 {
      margin: 0 0 18px 0;
      text-align: center;
      line-height: 30px;
    }

    #login .default-text {
      font-family: serif;
      font-style: italic;
      margin-bottom: 1.5em;
      font-size: 17px;
      display: block;
      text-align: center;
      margin-top: 33px;
    }

    form#login_form {
      border-top: 1px solid #e0e0e0;
      padding-top: 25px;
      margin-top: 25px;
      font-size: 80%;
      margin-bottom: 35px;
    }

    form#login_form label {
      color: #777;
    }

    #login_form #password {
      font-size: 14px;
      padding: 4px 6px;
      border: 1px solid #ccc;
      outline: none;
      background: #e9e9e9;
      background: -moz-linear-gradient(top, #ececec 0%, #fff 100%);
      background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ececec), color-stop(100%,#fff));
    }

    #login_form label, #login_form input {
      float: left;
      margin-right: 3px;
    }

    #footer {
      margin-top: 15px;
      margin-bottom: 40px;
      font-size: 13px;
      color: #666;
      text-shadow: 0 1px rgba(255,255,255,0.4);
      text-align: center;
    }

    #powered {
      color: #9c9c9c;
      display: block;
      margin-top: 15px;
    }

    #powered a {
      background-repeat: no-repeat;
      background-image: url(data:image/png;base64,);
      padding-left: 21px;
      color: #9c9c9c;
      text-decoration: none;
    }

    #powered a:hover {
      text-decoration: underline;
    }

    .errors {
      background: #feebeb;
      padding: 4px 5px;
      font-size: 15px;
      color: #7e1412;
      display: block;
      margin-bottom: 15px;
    }

    .errors ul {
      list-style: none;
      padding: 0;
      margin: 0;
    }

    .center {
      text-align: center;
      display: block;
      font-size: 17px;
    }
    
    
    
    /*! Override Shopify Dammit
---------------------------------------------*/
#container,#login{width:100%;margin-top:0}
#login{background:transparent;border:none;box-shadow:none;border-radius:none;padding:0}
#login h1{}
form#login_form{width:200px;margin:0 auto;margin-top:0}
form#login_form label{float:none;display:block;margin: 0 auto;text-align: center;font-size: 18px;margin-bottom:20px;color:#EBE9D2;}
#login_form input{float:none;font-size: 18px;color: black;margin:0;}
div.actions{width:200px;text-align:center;margin-top:20px}           
#login_form #password{float:none;margin: 0 auto;text-align: left;font-size: 30px;width:180px;padding:5px 10px;color:#000}
#powered,#owner{color:#444;width:400px;margin:0 auto;margin: 10px auto;font-size: 18px;}
#powered a,#owner a{color:#000}
#footer{margin-top:20px 0;}
#owner{display:none;}


     
/*! General
---------------------------------------------*/
* { margin: 0; padding: 0; }

html { overflow-y: scroll; min-height: 100%; margin: 0 0 1px; font-size: 100.01%; -webkit-tap-highlight-color: rgba(0,0,0,0); }
body { min-height: 100%; background:#ffffff url(https://cdn.shopify.com/s/files/1/0274/0369/files/AW16-Shop-Background.jpg) no-repeat center center fixed; -webkit-background-size: cover; -moz-background-size: cover; -o-background-size: cover;  background-size: cover;  font-size: 62.5%; }

body, textarea, input, select, option, button { color: #EBE9D2; font-family: 'Yanone Kaffeesatz', Arial, Helvetica, sans-serif; line-height: 1.05; }
li, dt, dd, p, figure, th, td, caption, legend, pre { font-size: 2em; } li *, dt *, dd *, p *, figure *, th *, td *, legend * { font-size: 1em; }
ul, ol, dl, p, figure, table, pre, h1, h2, h3, h4, h5, h6, legend { margin-bottom: 15px; }

article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section, div { display: block; }


/*! Layout
---------------------------------------------*/

#container{}

#root { width: 825px; margin: 0 auto;text-shadow: 0 1px rgba(84, 84, 84, 0.4); }
  #top { position: relative; }
    #top h1 { visibility:visible; display: block; width: auto; height: 160px; margin: 0 auto 17px; }
      #top h1 a { display: block; overflow: hidden; width: 100%; height: 100%; background: url(http://comingsoon.smarterdev.com/logos/zddzmyshopifycom.gif) no-repeat; background-position: center; text-indent: -3000em; }
    #skip { position: fixed; top: 0; left: -10000px; }
      #skip a:focus, #skip a:active { display: block; position: absolute; left: 10000px; top: 0; z-index: 10001; width: 200px; margin: 15px 0 0 15px; padding: 4px 8px; background: #333; color: #fff; font-weight: 500; font-size: 1.1em; text-align: center; }
    #nav {margin: 0 0 60px 0;}
      #nav ul { list-style: none; margin: 0; padding: 0; }
        #nav ul li { }
        #nav ul li a { }
          #nav ul li.active a { cursor: default; }
      #nav em, #nav h2 { position: absolute; left: -3000em; top: -3000em; }
  #content { text-align: center; }
    #content > p { margin-bottom: 0; font-size: 4.8em; }
  #custom-footer { padding: 64px 0 0; text-align: center; }
    #custom-footer ul { list-style: none; padding: 0; }
      #custom-footer li { display: inline; padding: 0 19px 0 26px; background: url(http://cdn.shopify.com/s/files/1/0000/0210/t/50/assets/li-a.gif) 0 13px no-repeat; font-size: 2em; }
        #custom-footer li:first-child { padding-left: 0; background: none; }
        #custom-footer li:last-child { padding-right: 0; }
        #custom-footer li.last-child { padding-right: 0; }
      #custom-footer li a { color: #EBE9D2; }

    h1 img {margin-top:20px!important;}
    .default-text { font-size:26px!important; color:#000!important; font-style:normal!important; font-weight:bold!important; font-family:Verdana!important;}
    p, .default-text {margin-top:0px!important; padding-top:0px!important; }
    h1, img {margin-bottom:0px!important; padding-bottom:0px!important; }
form#login_form {
    border-top: 0;
}

/*! Headers
---------------------------------------------*/
h1, h2, h3, h4, h5, h6 { line-height: 0.5; }
h1 { font-size: 2.2em; }
h2 { font-size: 2.0em; color: #666; }
h3 { font-size: 1.8em; }
h4 { font-size: 1.6em; }
h5 { font-size: 1.5em; }
h6 { font-size: 1.4em; }
    
  </style>
</head>
<body>

  <div id='container'>
    <div id='login'>
      <div>
        <h1><img src="http://zddzlondon.com/wp-content/uploads/2014/07/ZDDZ-London-Logo.png" /></h1>
          <p><span class="default-text">COMING SOON</span></p>
      </div>
      <div>
{% form 'storefront_password' %}
{{ form.errors | default_errors }}
<input type="password" name="password">
{% endform %}
      </div>
    </div>


</body>
</html>
