{
  "env": {
    "browser": true,
    "jasmine": true,
    "es6": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "restParams": true,
    "spread": true,
    "templateStrings": true
  },
  "globals": {
    "_": true,
    "R": true,
    "$": true
  },
 "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": 0,
    "curly": 2,
    "quotes": [
      0,
      "single"
    ],
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "camelcase": 0,
    "no-unused-vars": 1,
    "no-var": 0,
    "object-shorthand": [
      1,
      "properties"
    ],
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "no-unused-expressions": [
      2,
      {
        "allowShortCircuit": true,
        "allowTernary": true
      }
    ]
  }
}
